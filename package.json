{
  "name": "@storybook/csf",
  "version": "0.0.2-alpha.0",
  "description": "Component Story Format (CSF) utilities",
  "keywords": [
    "storybook",
    "component story format",
    "csf",
    "stories"
  ],
  "homepage": "https://github.com/ComponentDriven/csf",
  "bugs": {
    "url": "https://github.com/ComponentDriven/csf/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ComponentDriven/csf.git"
  },
  "license": "MIT",
  "files": [
    "dist/**/*",
    "README.md",
    "*.js",
    "*.d.ts"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup ./src/index.ts --format esm,cjs --dts",
    "check": "tsc",
    "lint": "eslint src --ext .ts",
    "test": "jest",
    "release": "yarn build && auto shipit"
  },
  "eslintConfig": {
    "extends": [
      "@storybook/eslint-config-storybook"
    ],
    "rules": {
      "jest/expect-expect": [
        "warn",
        {
          "assertFunctionNames": [
            "expect",
            "expectTypeOf"
          ]
        }
      ]
    }
  },
  "prettier": "@storybook/linter-config/prettier.config",
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src"
    ]
  },
  "dependencies": {
    "expect-type": "^0.14.2",
    "lodash": "^4.17.15",
    "type-fest": "^2.19.0"
  },
  "devDependencies": {
    "@auto-it/released": "^10.37.1",
    "@storybook/eslint-config-storybook": "^2.1.0",
    "@types/jest": "^29.2.0",
    "@types/lodash": "^4.14.149",
    "@types/node": "^18.11.0",
    "@typescript-eslint/parser": "^4.33.0",
    "auto": "^10.31.0",
    "common-tags": "^1.8.0",
    "eslint": "^6.7.1",
    "jest": "^29.2.0",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "tsup": "^6.3.0",
    "typescript": "^4.8.4"
  },
  "auto": {
    "plugins": [
      "npm",
      "released"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
